<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appenders>
		<console name="console" target="SYSTEM_OUT">
			<patternlayout pattern="%date [%t] %p %-5l - %msg%n" />
		</console>
	</appenders>
	<loggers>
		<root level="error">
			<appender-ref ref="console"/>
		</root>
	</loggers>
</configuration>